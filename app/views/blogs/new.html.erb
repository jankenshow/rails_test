<div id="article_wrap">
  <h1>New Blog</h1>
  
  <% if @blog.errors.any? %>
    <div>
      <ul>
        <% @blog.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
<% end %>

  <%= render 'form', blog: @blog %>

<%= link_to 'Back', root_path %>

<%= render 'layouts/sidebar_ranking' %>
</div>
